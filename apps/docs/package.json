{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "storybook build -o build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf node_modules .svelte-kit .turbo build",
    "dev": "storybook dev -p 5174 --no-open",
    "lint": "eslint . --max-warnings 0",
    "prepare": "svelte-kit sync",
    "preview": "http-server build --port 4174",
    "test": "vitest run",
    "test:ci": "vitest",
    "test:unit:ui": "vitest --ui"
  },
  "dependencies": {
    "@internationalized/date": "^3.8.2",
    "assets": "*",
    "bits-ui": "^2.8.7",
    "locale": "*",
    "tailwind-variants": "^1.0.0",
    "typesafe-i18n": "^5.26.2",
    "ui": "*",
    "utils": "*",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-svelte-csf": "^5.0.3",
    "@storybook/blocks": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/svelte": "^8.6.14",
    "@storybook/sveltekit": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/enhanced-img": "^0.6.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-plugin-storybook": "^0.12.0",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "storybook": "^8.6.14",
    "svelte": "^5.34.7",
    "svelte-check": "^4.2.2",
    "tailwind-config": "*",
    "tailwindcss": "^4.1.10",
    "turbo-utils": "*",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
